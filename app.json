{
  "expo": {
    "name": "IDWeather",
    "slug": "IDWeather",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.johntribolta.IDWeather"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "IDWeather",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0d832fcd-ea9e-46ea-8443-1604f47b6608"
      }
    },
    "owner": "johntribolta"
  }
}
